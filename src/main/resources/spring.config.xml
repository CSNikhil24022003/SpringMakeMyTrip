<?xml version="1.0" encoding="UTF-8"?>
 <beans xmlns="http://www.springframework.org/schema/beans"  
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
   xmlns:p="http://www.springframework.org/schema/p"  
   xsi:schemaLocation="http://www.springframework.org/schema/beans  
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
 <bean id="Bus" class="anudip.org.Bus"></bean>
 <bean id="Train" class="anudip.org.Train"></bean>
 <bean id="AirWays" class="anudip.org.AirWays"></bean>
<bean id="MakeMyTrip" class="anudip.org.MakeMyTrip">
	<property name="trip" ref="Bus"></property>
</bean>

</beans>